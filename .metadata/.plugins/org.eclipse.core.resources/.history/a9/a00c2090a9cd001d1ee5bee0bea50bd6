package com.studyopedia;

import javax.swing.JOptionPane;

public class Menu{
	public static void main(String args[]) {
		String heading = JOptionPane.showInputDialog("Menu - Press 1 to log in and 2 to sign up");
		int number = Integer.parseInt(heading);
		if(number == 1) {
			JOptionPane.showMessageDialog(null, "You chose: "+heading);
		}
		else if (number == 2) {
			JOptionPane.showMessageDialog(null, "You chose: "+heading);
		}
		else {
			JOptionPane.showMessageDialog(null, "idk");
		}
	}
}